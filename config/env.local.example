# ============================================
# LOCAL OVERRIDES
# ============================================
# This file is for personal development settings
# It will override values from .env.development
# NEVER commit this file to git

# Example: Use different port
# PORT=8001

# Example: Use real API key for testing
# GEMINI_API_KEY=your-personal-test-key

# Example: Use external Redis
# REDIS_URL=redis://localhost:6379/5

# Example: Personal rate limits
# RATE_LIMIT_PER_MINUTE=9999

# Example: Use different storage path
# STORAGE_PATH=./my-custom-storage

# Example: Enable additional logging
# LOG_LEVEL=DEBUG

# Example: Use different LiteLLM URL
# LITELLM_URL=http://localhost:4001

# Example: Disable certain features
# LANGFUSE_ENABLED=false
# ENABLE_METRICS=false
